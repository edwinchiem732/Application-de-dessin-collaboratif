{
  "name": "client",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ng serve --open",
    "start:electron": "ng build --base-href ./ && electron .",
    "build": "ng build",
    "build:electron": "ng build --prod && electron-packager . --asar=true --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build",
    "deploy": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --config ../.prettierrc.yaml --ignore-path ../.prettierignore --write \"src/**/*\"",
    "test": "ng test",
    "coverage": "ng test --code-coverage --sourceMap=true",
    "electron": "electron .",
    "electron-build": "ng build --prod",
    "lint": "ng lint --format stylish",
    "lint:fix": "npm run lint -- --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.0",
    "@angular/cdk": "^13.2.0",
    "@angular/common": "13.2.0",
    "@angular/compiler": "13.2.0",
    "@angular/core": "13.2.0",
    "@angular/forms": "13.2.0",
    "@angular/material": "^13.2.0",
    "@angular/platform-browser": "13.2.0",
    "@angular/platform-browser-dynamic": "13.2.0",
    "@angular/router": "13.2.0",
    "@types/howler": "^2.2.7",
    "howler": "^2.2.3",
    "mock-socket": "^9.0.6",
    "mongodb-memory-server": "^8.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^13.2.0",
    "@angular/compiler-cli": "13.2.0",
    "@angular/localize": "^13.2.0",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@types/jasmine": "~3.8.0",
    "@types/luxon": "^2.0.4",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "electron": "^16.0.8",
    "electron-packager": "^15.4.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "moment": "^2.29.1",
    "ngx-moment": "^6.0.2",
    "prettier": "^2.3.2",
    "socket.io-client": "^4.4.0",
    "typescript": "~4.5.5"
  }
